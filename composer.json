{
    "name": "axeldotdev/content-blocks",
    "description": "A Laravel Nova field for flexible content blocks.",
    "keywords": [
        "laravel",
        "nova",
        "content",
        "blocks"
    ],
    "homepage": "https://github.com/axeldotdev/content-blocks",
    "license": "MIT",
    "authors": [
        {
            "name": "Axel Charpentier",
            "email": "axelcharpentier0@icloud.com",
            "role": "Developer",
            "homepage": "https://github.com/axeldotdev"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Axeldotdev\\ContentBlocks\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Axeldotdev\\ContentBlocks\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
